                        BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid Width="12" Height="15">
                            <Path
                                x:Name="maxpath"
                                Width="10"
                                Height="10"
                                VerticalAlignment="Center"
                                Data=" M2,2 L2,12 12,12 12,2 z M0,0 L14,0 14,14 0,14 z"
